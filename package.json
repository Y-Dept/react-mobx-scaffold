{
  "name": "rmb-scaffold",
  "version": "1.0.0",
  "description": "react mobx react-router scaffold",
  "scripts": {
    "start": "npm run dev:local | npm run server",
    "dev:local": "cross-env NODE_ENV=development LOGIN_HOST=test PROXY=local npx webpack-dev-server --hot --mode=development",
    "dev:remote": "cross-env NODE_ENV=development LOGIN_HOST=test PROXY=remote npx webpack-dev-server --hot --mode=development",
    "server": "cross-env NODE_ENV=development npx nodemon --exec npx babel-node ./mockServer/app.js",
    "build:test": "cross-env NODE_ENV=production LOGIN_HOST=test npx webpack --progress --hide-modules --mode=production",
    "build:production": "cross-env NODE_ENV=production npx webpack --progress --hide-modules --mode=production",
    "production": "npm run build:production",
    "e2e": "NODE_ENV=production npx babel-node e2e/runner.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "author": "superwf@gmail.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/superwf/react-mobx-scaffold.git"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browserslist": "^4.0.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.9.1",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "jest-environment-enzyme": "^6.0.0",
    "jest-enzyme": "^6.0.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.5",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.5",
    "postcss-syntax": "^0.28.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-property-sort-order-smacss": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack-cli": "^3.0.4"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "ant-design-pro": "^2.0.0-beta.2",
    "antd": "^3.7.0",
    "axios": "^0.18.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "chromedriver": "^2.40.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "cross-spawn": "^6.0.5",
    "dexie": "^2.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise": "^4.2.4",
    "fetch-mock": "^6.4.4",
    "fxios": "^0.5.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.11.0",
    "less": "^3.0.4",
    "load-polyfill": "^0.2.1",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.10",
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.2",
    "mobx-react-router": "^4.0.4",
    "moment": "^2.22.2",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.21",
    "nightwatch-helpers": "^1.2.0",
    "opn": "^5.3.0",
    "path-to-regexp": "^2.2.1",
    "pluralize": "^7.0.0",
    "portfinder": "^1.0.13",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-loadable": "^5.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-transition": "^1.2.1",
    "selenium-server": "^3.12.0",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^3.1.4",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/component/**/*.{js,jsx,mjs}",
      "app/page/**/*.{js,jsx,mjs}",
      "app/store/**/*.{js,jsx,mjs}",
      "app/storeProp/**/*.{js,jsx,mjs}",
      "app/mixin/**/*.{js,jsx,mjs}",
      "app/util/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/__test__/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testEnvironment": "enzyme",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|less)$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/).+(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^component/(.+)$": "<rootDir>/app/component/$1",
      "^util/(.+)$": "<rootDir>/app/util/$1",
      "^store/(.+)$": "<rootDir>/app/store/$1",
      "^storeProp/(.+)$": "<rootDir>/app/storeProp/$1",
      "^app/(.+)$": "<rootDir>/app/$1",
      "history/createBrowserHistory": "<rootDir>/node_modules/history/createMemoryHistory"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node"
    ]
  }
}
