{
  "name": "rmb-scaffold",
  "version": "1.0.0",
  "description": "react mobx react-router scaffold",
  "scripts": {
    "start": "npm run server | npm run dev:local",
    "dev:local": "cross-env NODE_ENV=development LOGIN_HOST=test PROXY=local npx webpack-dev-server --hot --mode=development",
    "dev:remote": "cross-env NODE_ENV=development LOGIN_HOST=test PROXY=remote npx webpack-dev-server --hot --mode=development",
    "server": "cross-env NODE_ENV=development npx nodemon --exec npx babel-node ./mockServer/app.js",
    "build:test": "cross-env NODE_ENV=production LOGIN_HOST=test npx webpack --progress --hide-modules --mode=production",
    "build:production": "cross-env NODE_ENV=production npx webpack --progress --hide-modules --mode=production",
    "production": "npm run build:production",
    "e2e": "npx babel-node e2e/runner.js",
    "test": "jest",
    "coverage": "jest --coverage && node jest/openCoverage.js"
  },
  "author": "superwf@gmail.com",
  "license": "ISC",
