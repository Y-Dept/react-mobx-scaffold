{
  "src/component/*": { "type": "component" },
  "src/page/*": { "type": "page" },
  "src/store/*.js": { "type": "store" },
  "src/storeProp/*.js": { "type": "storeProp" },
  "src/mixin/*.js": { "type": "mixin" },
  "src/locale/*.js": { "type": "locale" },
  "src/style/*.less": { "type": "style" },
  "src/tool/*.js": { "type": "tool" },
  "__test__/*": { "type": "test" },
  "e2e/spec/*.js": { "type": "spec" }
}
